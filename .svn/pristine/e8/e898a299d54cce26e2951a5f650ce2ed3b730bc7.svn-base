		var collected = getCookie("collected{$info.id}");
		if(collected != ""){
			if(collected == "yes"){
				$('.receive-collect-img').children('span').children('i').removeClass('fa-heart-o');
				$('.receive-collect-img').children('span').children('i').addClass('fa-heart');
				$('.receive-collect-img').children('span').children('i').children("span").removeClass("hide1 fa-heart-o");
				$('.receive-collect-img').children('span').children('i').children("span").addClass(' inline fa-heart');
			}else{
				$('.receive-collect-img').children('span').children('i').removeClass('fa-heart');
				$('.receive-collect-img').children('span').children('i').addClass('fa-heart-o');
				$('.receive-collect-img').children('span').children('i').children("span").removeClass("fa-heart inline fly");
				$('.receive-collect-img').children('span').children('i').children("span").addClass('fa-heart-o hide1');
			}
		}
		function getCookie(cname)
		{
		  var name = cname + "=";
		  var ca = document.cookie.split(';');
		  for(var i=0; i<ca.length; i++) 
		  {
		    var c = ca[i].trim();
		    if (c.indexOf(name)==0) return c.substring(name.length,c.length);
		  }
		  return "";
		}	

