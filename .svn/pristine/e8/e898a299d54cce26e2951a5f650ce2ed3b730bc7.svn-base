		if(getCookie("{$info.id}status") != ""){
			$(".receive-buttton").html(decodeURIComponent(getCookie("{$info.id}status")));
			$("#receive").attr('data-orderid',getCookie("{$info.id}orderid"));
			$(".receive-lingqu").attr('id',getCookie("{$info.id}buttonid"));
			//alert($(".receive-lingqu").attr('id'));
