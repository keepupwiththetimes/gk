		//领取
		$('#receive').click(function(){
			var url="{:U('Home/Member/doOrder')}";
			var product_id=$(this).attr('data-product_id');
			var platform_id=$(this).attr('data-platform_id');
			var data={product_id: product_id, platform_id: platform_id};
			var elm = $(this);
			$.post(url, data, function (jsonData) {
					var orderid = jsonData.orderId;
					$(".loaders").hide();
					if (jsonData.state==1 || jsonData.state==2) {
						
						showMsg("领取成功~<br>进入订单详情中...");
				 		setTimeout(function(){
				 			var backurl = "{:U('Home/Product/checkProductStatus')}"; 
							var backdata = {
								id:"{$info.id}",
								click_num:"{$info.click_num}",
							}
							$.post(backurl,backdata,function(response){
								var response = $.parseJSON(response);

								document.cookie="{$info.id}status="+encodeURIComponent(response.name);
								document.cookie="{$info.id}orderid="+orderid;
								document.cookie="{$info.id}buttonid="+response.id;
								$(".receive-buttton").html(response.name);
								elm.attr("data-orderid",orderid);
								$(".receive-lingqu").attr("id",response.id);
								$("#point").hide();
				 				do_url("{:U('Home/Order/orderDetail/id/','','')}/"+jsonData.orderId);
							});
				 		},1000);
					} else if (jsonData.state==-1) {
						login('receive')
					} else {
							showMsg(jsonData.msg);
					}
			}, 'json');
		});
	});
	
