window.onpageshow = function(){
		
		var first_state = window.localStorage.getItem('first_state');
		var second_state = window.localStorage.getItem('second_state');	
	
	if (window.localStorage.getItem('second_state')) {
					
					if (first_state == second_state) {
						window.localStorage.ty = 'no';
					}else{
						
						if (first_state=='fa fa-heart-o') {
							window.localStorage.ty = 'sc';
						} else{
							window.localStorage.ty = 'qx';
						}

						
					}
					
				} else{
				
				window.localStorage.ty = 'no';
				window.localStorage.coll = 'collect';				
				}
				
				window.localStorage.removeItem('second_state');
			
//			alert(first_state);alert(second_state);alert(window.localStorage.ty);
			
}


