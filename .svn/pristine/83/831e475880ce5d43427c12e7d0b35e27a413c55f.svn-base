@charset "UTF-8";

html,body{
	margin: 0;
	padding: 0;
	width: 100%;
	-webkit-tap-highlight-color: rgba(255,255,255,0);
}

.pika-single {
    z-index: 9999;
    display: block;
    position: relative;
    width: 100%;
    color: #333;
    background: #fff;
    border: none;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}

/*.pika-single.is-hidden {
    display: none;
}*/

.pika-single.is-bound {
    position: absolute;
    left: 17%;
    top: 51.5%;
    width: 66%;
}

.pika-title {
    position: relative;
    text-align: center;
    background-color: #5BA2F4;
    left: 0.5px;
}

.pika-label {
    display: inline-block;
    *display: inline;
    position: relative;
    z-index: 9999;
    overflow: hidden;
    margin: 0;
    padding: 5px 3px;
    font-size: 14px;
    line-height: 20px;
    font-weight: bold;
    color: white;
}
.pika-title select {
    cursor: pointer;
    position: absolute;
    z-index: 9998;
    margin: 0;
    left: 0;
    top: 5px;
    filter: alpha(opacity=0);
    opacity: 0;
}

.pika-prev,
.pika-next {
    display: block;
    cursor: pointer;
    position: relative;
    outline: none;
    border: 0;
    padding: 0;
    width: 20px;
    height: 30px;
    background-color: transparent;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 75% 75%;
    white-space: nowrap;
    text-indent: 100%;
    overflow: hidden;
    /*opacity: .5;*/
    *position: absolute;
    *top: 0;
    
}

.pika-prev:hover,
.pika-next:hover {
    opacity: 1;
}

.pika-prev,
.is-rtl .pika-next {
    float: left;
    background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAeAgMAAABRfNb4AAAACVBMVEUAAACPj4/////qt+jxAAAAAnRSTlMAjwuUU4kAAAAtSURBVAjXYyADOIAIxgYQyQYmpcBkFohkXAUi2aAkRAQiC1WJ0MXYADWNVAAAmScKTcU+S4EAAAAASUVORK5CYII=');
    *left: 0;
}

.pika-next,
.is-rtl .pika-prev {
    float: right;
    background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAeAgMAAABRfNb4AAAACVBMVEUAAACPj4/////qt+jxAAAAAnRSTlMAjwuUU4kAAAArSURBVAjXYyAZMIJJJgcwOQFMLgGTK8HkKgcwOQGJdEDILkHS5QAxjXQAAAYzCJ2M7PT/AAAAAElFTkSuQmCC');
    *right: 0;
}

.pika-prev.is-disabled,
.pika-next.is-disabled {
    cursor: default;
    opacity: .2;
}

.pika-select {
    display: inline-block;
    *display: inline;
}

.pika-table {
    width: 100%;
    border-collapse: collapse;
    border-spacing: 0;
    border: 0;
}

.pika-table th,
.pika-table td {
    width: 14.285714285714286%;
    border-left: 1px solid #EFEFEF;
    border-bottom: 1px solid #EFEFEF;
}

.pika-table th {
    color: #8A9CA9;
    font-size: 0.6rem;
    line-height: 25px;
    font-weight: bold;
    text-align: center;
    
}


.pika-button {
    cursor: pointer;
    display: block;
    outline: none;
    border: 0;
    margin: 0;
    width: 100%;
    /*padding: 5px;*/
	/*padding-top: 25%;
	padding-bottom: 25%;*/
	font-weight: bold;
	font-family: arial;
    font-size: 15px;
	height: 100%;
    /*color: #5d5d5d;*/
   color: #999;
    line-height: 15px;
    text-align:center;
    background: white;
}
.signing{
	   color: #999;
}
.is-today .pika-button {
    color: white;
    font-weight: bold;
    background-color: #4CDB6F;
}

.is-selected .pika-button {
    font-weight: bold;
    background: white;
    /*box-shadow: inset 0 1px 3px #178fe5;*/
/*    border-radius: 3px;*/
}

.is-disabled .pika-button {
    pointer-events: none;
    cursor: default;
    color: #999;
    opacity: 0.3;
}




.pika-title button:nth-child(3){
		margin-left: 3%;
}

.pika-title button:nth-child(4){
		position: relative;
		left: -14px;
}

.pika-title button:nth-child(5){
		margin-right: 3%;
}

.pika-title button:nth-child(6){
		position: relative;
		left: 14px;
}

.pika-table>thead>tr{
	background-color: #E6F1FD;
}
