//--------------------------------detail(详情)页----------------------------

.carousel-indicators li{
    background-color: rgba(0,0,0,0.3);
}
.carousel-indicators .active{
    background-color:@bgColor;
}
#detail-container{
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    #carousel-example-generic{
        width: 100%;
        .banner-img{
            width: 100%;
        }
    }
    .detail-main{
        width: @width; 
        border: 2px solid @bgColor;  
        position: relative;
        margin-left: @left;
        margin-top: 40px;
        margin-bottom: 2rem;
        box-sizing: border-box;
        .role{
            font-size: 2rem;
            color: @bgColor;
            display: inline-block;
            background-color: @color;
            padding: 0.5rem 3rem;
            letter-spacing: 0.5rem;
            font-family: "Trebuchet MS";
            font-weight: bold;
            position: absolute;
            left:50%;
            margin-left: -7.75rem;
            top: -2.5rem;
        }
        ul{
            width: 93.34%;
            margin-top: 2rem;
            font-size: 14px; 
            line-height: 24px;
            color: @fontColor;
            li{
                margin-left: 4.83%;
                color:@bgColor;
                font-size: 1.4rem;
                margin-top: 3rem;
                p{
                    font-size: 1.4rem;
                    color: @fontColor;
                }
            }
        }
        strong{
           height: 0;
           width: 100%;
           border: 1px dashed @bgColor;
           display: block;
           margin: 25px 0 10px;
        }
        .store{
//          width: 84.44%;
            margin-left: 5.5%;
            margin-bottom: 1rem;
            i{
                color: @bgColor;
                font-size: 2.2rem;
            }
            a{
                color: @bgColor;
                font-size: 1.4rem;
                display: inline-block;
                margin-left: 1rem;
                text-decoration: none;
            }
        }
    }
    .empty-detail{
        width: 100%;
        height: 5px;
    }
}



//----------------------------------登录页----------------------------------
#login-box{
    width: 100%;
//  height: 100%;
    min-height: 100%;
    background-color: @loginColor;
    padding-bottom: 2rem;
    .header{
        width: 100%;
        height: 10rem;
        position: relative;
        background-color: @bgColor;
        margin-bottom: 2rem;
        p{
            font-size: 2rem;
            color: @color;
            text-align: center;
            line-height: 10rem;
        }
        a{  
            display: block;
            position: absolute;
            top: 2.4rem;
            left: @left;
            
                padding: 20px;
                padding-left: 0;
            i{
                color: @color;
                font-size: 1.8rem;
                box-sizing: border-box;
            }
        }
    }
    .center{
        width: 100%;
        height: 160px;
        background-color: @eColor;
        border-bottom: 1px solid @borderColor;
        border-top: 1px solid @borderColor;
        .center-box{
            padding-top: 3rem;
            div{
                width: 8rem;
                height: 8rem;
                border: 2px solid @color;
                border-radius: 4rem;
                box-sizing: border-box;
                margin: 0 auto;
                overflow: hidden;
                img{
                    width: 100%;
//                  padding: 3px;
                }
            }
            p{
                width: 100%;
                text-align: center;
                color: @fontColor;
                font-size: 1.6rem;
            }
        }
            
    } 
    .number{
      width: 100%;
      background-color: @color;
      p{
          border-bottom: 1px solid @borderColor;
          margin: 0;
          height:6rem;
          line-height: 6rem;
          font-size: 1.6rem;
          padding-left: @left;
          overflow: hidden;
          input{
              margin-top: -6px;
              width: 180px;
              text-indent: 1rem;
          }
          span{
              float: right;
              color: @color;
              font-size: 1.6rem;
              background-color: @gray;
              margin-right: @left;
              height: 3rem;
              line-height: 3rem;
              margin-top:1.5rem;
              border-radius: 1.5rem;
              padding: 0 1rem;
          }
          .active-login{
              background-color: @bgColor;
          }
      } 
       
    } 
    .submitBtn{
        width: @width;
        margin-left: @left;
        margin-top: 2rem;
        display: inline-block;
        font-size: 2.4rem;
        font-family: "Trebuchet MS";
        font-weight: bold;
        height: 4rem;
        line-height: 4rem;
        border-radius: 2rem;
        color: @color;
        background-color: @gray;
        text-align: center;
    }
    .active-login1{
        background-color: @bgColor;
    } 
    ul{
        width: @width;
        margin-left: @left;
        margin-top: 3rem;
        li{
            color: @bgColor;
            font-size: 14px;
            span{
                color: @fontColor;
                font-size: 14px;
            }
        }
    }
}

//--------------------------------address/address1(地址)页----------------------------

#address-box{
    width: 100%;
    height: 100%;
    background-color: @loginColor;
    padding-bottom: 2rem;
    .header{
        width: 100%;
        height: 100px;
        background-color: @bgColor;
        margin-bottom: 20px;
        position: relative;
        p{
            font-size: 20px;
            color: @color;
            text-align: center;
            line-height: 100px;
        }
        a{  
            display: block;
            position: absolute;
            top: 24px;
            left: @left;
            
                padding: 20px;
                padding-left: 0;
            i{
                color: @color;
                font-size: 18px;
                box-sizing: border-box;
            }
        }
    }
    .address-add{
        width: 100%;
        margin: 0 auto;
        background-color: @color;
        p{
            border-bottom: 1px solid @borderColor;
            width: 100%;
            margin: 0;
            height:60px;
            line-height: 60px;
            font-size: 16px;
            overflow: hidden;
            padding-left: @left;
            .input{
                margin-top: -6px;
                width: 200px;
                text-indent: 10px;
            }
            .select{
                font-size: 16px;
                margin-top: 0;
                text-indent: 10px;
            }
        }
        .sex{
            font-size: 1.6rem;
            padding-left: 0;
