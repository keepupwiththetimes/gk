@bgColor:#ee782c;//主题色
@color:#fff;
@fontColor:#595757;
@aColor1:#898989; 
@menuColor:#f2965b;
@loginColor:#f3f3f3;//登录页大背景色
@width:93.4%;
@left:3.3%;//离左边的距离
@eColor:#edd;//用户账号背景色
@borderColor:#9f9fa0;//登录页面边框颜色
@gray:lightgray;//登录页面置灰

*{
    border: 0;
    margin: 0;
    padding: 0;
    font-size: 10px;
}
p{
    margin: 0;
}
.picker-item{
    font-size: 16px;
}
.gray{
    -webkit-filter: grayscale(100%);
    -moz-filter: grayscale(100%);
    -ms-filter: grayscale(100%);
    -o-filter: grayscale(100%);
    filter: grayscale(100%);
    filter: gray;
}
.getLoad{
    position: absolute;
    bottom: 5rem;
    width: 100%;
    span{
        font-size: 14px;
        width: 100%;
        color: @bgColor;
        text-align: center;
    }
}
body,html{
    width: 100%;
    height: 100%;
    -moz-user-select:initial; 
    -webkit-user-select:inherit;
    -ms-user-select:inherit; 
    -khtml-user-select:inherit; 
    user-select:inherit;
//  background-color: #eee;
}

a{
    color: @aColor1;
    text-decoration: none;
}

hr{
    margin: 5px 0;
    height: 1px;
    width: 100%;
    background-color: #c9c7c7;
}

.am-icon-btn{
    color: @bgColor;
    position: fixed;
    left: @left;
    bottom: 2rem;
    z-index: 99999;
}
.am-icon-btn:focus, .am-icon-btn:hover{
    color: @bgColor;
    background-color: #eee;
}

*+address, *+blockquote, *+dl, *+fieldset, *+figure,  *+ol, *+p, *+pre, *+ul{
    margin-top: 0;
}
#amz-go-top{
    display: none;
}

.list-left p{
    height: 12px;
    width: 30%;
    position: absolute;
    left: -9%;
    top: 12%;
    background-color: rgba(255,59,4,.5);
    text-align: center;
    color: @color;
    line-height: 13px;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

#logout{
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.4);
    position: fixed;
    top: 0;
    z-index: 99999999;
    display: none;
    .logout-box{
        width: @width;
        height: 240px;
        background-color: @color;
        border: 2px solid @bgColor;
        position: absolute;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        margin: auto;
        border-radius: 20px;
        .text{
            width: 100%;
            text-align: center;
            height: 80px;
            line-height: 80px;
            font-size: 2rem;
            font-weight: bold;
            color: @bgColor;
            border-bottom: 1px solid @borderColor;
        }
        .number{
          width: 100%;
          background-color: @color;
          p{
              border-bottom: 1px solid @borderColor;
              margin: 0;
              height:4rem;
              line-height: 4rem;
              font-size: 1.4rem;
              padding-left: @left;
              overflow: hidden;
              input{
                  margin-top: -6px;
                  width: 180px;
                  text-indent: 1rem;
              }
              span{
                  float: right;
                  color: @color;
                  font-size: 1.4rem;
                  background-color: @gray;
                  margin-right: @left;
                  height: 2.4rem;
                  line-height: 2.4rem;
                  margin-top:0.8rem;
                  border-radius: 1rem;
                  padding: 0 1rem;
              }
              .active-login{
                  background-color: @bgColor;
              }
          } 
        } 
        .submitBtn{
            width: @width;
            margin-left: @left;
            margin-top: 2.5rem;
            display: inline-block;
            font-size: 1.8rem;
            font-family: "Trebuchet MS";
            font-weight: bold;
            height: 3rem;
            line-height: 3rem;
            border-radius: 3rem;
            color: @color;
            background-color: @gray;
            text-align: center;
        }
        .active-login1{
            background-color: @bgColor;
        } 
        .closeLogin{
            color: @bgColor;
            font-size: 20px;
            position: absolute;
            right: 3%;
            top: 2%;
            border: 1px solid @bgColor;
            width: 26px;
            height: 26px;
            line-height: 23px;
            text-align: center;
            border-radius: 13px;
        }
    }
}

