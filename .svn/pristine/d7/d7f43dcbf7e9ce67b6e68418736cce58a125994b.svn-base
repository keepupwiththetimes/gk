.users-list > li img {
    border-radius: 0%;
}

.users-list > li {
    width: 50%;
}

.main-header .sidebar-toggle {
    font-family: inherit;
}

.main-header .sidebar-toggle:before {
    content: "";
}

.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
    background-color: #ECF0F5;
}

.content {
    padding: 0px;
}

.nav-tabs > li.active {
    border-bottom: 2px solid #3c8dbc;
}

.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
    background: none;
    border: none;
}

ul.nav-tabs {
    border-bottom: 3px solid #ddd;
}

li .users-list-name {
    text-align: left;
    background: #E0E0E0;
}

span.users-list-date {
    text-align: left;
    background: #E0E0E0;
}

.shaixuan_div {
    float: left;
    margin-left: 1%;
}

.shaixuan_div_3 {
    width: 64%;

}

.shaixuan_div_1 {
    width: 18%;

}

.shaixuan_div_2 {
    width: 12%;

}

.shaixuan_ul {
    padding: 0;
    margin: 0;
}

.shaixuan_ul li {
    list-style: none;
    float: left;
    margin: 5px 5px;
}

.navbar-custom-menu .navbar-nav>li>a{
    padding-top: 9px;
    padding-bottom: 7px;
    text-align: center;
}