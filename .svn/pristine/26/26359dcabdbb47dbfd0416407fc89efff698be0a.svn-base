* {
  margin: 0;
  padding: 0;
}
body,
html {
  -moz-user-select: none;
  /*火狐*/
  -webkit-user-select: none;
  /*webkit浏览器*/
  -ms-user-select: none;
  /*IE10*/
  -khtml-user-select: none;
  /*早期浏览器*/
  user-select: none;
  background-color: #eee;
}
hr {
  width: 99%;
}
