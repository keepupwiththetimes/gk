/*媒体查询，适配移动端*/
@media only screen and (min-width: 100px) and (max-width: 768px) {
  body,
  html {
    width: 100%;
    height: 100%;
  }
  .container {
    width: 100%;
  }
  .swiper-slide .sale img {
    width: 100%;
  }
  .sale .sale-content {
    margin: 10%;
  }
  .sale .pra-one {
    color: #fff;
    font-size: 20px;
    font-weight: bolder;
  }
  .sale .pra-two {
    color: #fff;
    margin-top: -10px;
    font-size: 16px;
  }
  .sale .pra-two span {
    font-size: 20px;
    font-weight: bolder;
  }
  .sale .pra-two .dollar {
    font-size: 16px;
    font-weight: bolder;
  }
}
